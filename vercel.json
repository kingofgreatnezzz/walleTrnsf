{
  "build": {
    "env": {
      "REACT_APP_NETWORK_ID": "1",
      "REACT_APP_NETWORK_NAME": "Ethereum",
      "REACT_APP_CHAIN_ID": "0x1",
      "REACT_APP_CURRENCY_SYMBOL": "ETH",
      "REACT_APP_BLOCK_EXPLORER_URL": "https://etherscan.io"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 